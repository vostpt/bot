{
  "name": "vostpt-bot",
  "version": "1.0.0",
  "description": "VOST PT bot",
  "main": "./app.js",
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "moment": "^2.30.1",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "eslint": "^8.57.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/app.js",
